{{#if hasBlock}}
  {{yield
    (hash
      visibleProcessedColumns=visibleProcessedColumns
      cell=(
        component "models-table/cell"
        record=record
        index=index
        column=column
        sendAction=sendAction
        expandRow=expandRow
        collapseRow=collapseRow
        expandAllRows=expandAllRows
        collapseAllRows=collapseAllRows
        themeInstance=themeInstance
        expandedRowIndexes=expandedRowIndexes
        isExpanded=(exists-in expandedRowIndexes index)
      )
    )
  }}
{{else}}
  {{#each visibleProcessedColumns as |column|}}
    {{models-table/cell
      data=data
      record=record
      index=index
      column=column
      sendAction=sendAction
      expandRow=expandRow
      collapseRow=collapseRow
      expandAllRows=expandAllRows
      collapseAllRows=collapseAllRows
      themeInstance=themeInstance
      expandedRowIndexes=expandedRowIndexes
      isExpanded=(exists-in expandedRowIndexes index)
    }}
  {{/each}}
{{/if}}